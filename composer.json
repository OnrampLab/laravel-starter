{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "barryvdh/laravel-ide-helper": "^3",
        "beyondcode/laravel-dump-server": "^2.1",
        "calebporzio/parental": "^1.4",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": "^11.44",
        "laravel/horizon": "^5.31",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10",
        "nunomaduro/collision": "^8.1",
        "nwidart/laravel-modules": "^10.0",
        "onramplab/laravel-clean-architecture": "^11",
        "owen-it/laravel-auditing": "^13.7",
        "php-open-source-saver/jwt-auth": "^2.8",
        "spatie/laravel-ignition": "^2.9",
        "spatie/laravel-permission": "^6"
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/*"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "code:analysis": [
            "phpstan analyse"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "pest"
        ],
        "test:watch": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit-watcher watch --filter NoneTest < /dev/tty"
        ],
        "rector": [
            "rector process"
        ],
        "rector:dry-run": [
            "rector process --dry-run"
        ],
        "insights": [
            "phpinsights"
        ],
        "insights:fix": [
            "phpinsights fix"
        ],
        "phpcs:fix-pr": [
            "tools/scripts/phpcs-fix.sh pr"
        ],
        "phpcs:fix-all": [
            "PHP_CS_FIXER_IGNORE_ENV=true tools/scripts/phpcs-fix.sh all"
        ],
        "refactor": [
            "composer rector",
            "composer insights:fix",
            "composer phpcs:fix-all"
        ],
        "refactor:dry-run": [
            "composer rector:dry-run",
            "composer insights"
        ]
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "filp/whoops": "^2.17",
        "friendsofphp/php-cs-fixer": "^3.75",
        "larastan/larastan": "^2.11",
        "mockery/mockery": "^1.6",
        "nunomaduro/phpinsights": "^2.13",
        "onramplab/onr-phpcs-laravel": "^1.2",
        "pestphp/pest": "^2.36",
        "phpunit/phpunit": "^10.5",
        "rector/rector": "^0.17.0",
        "sebastian/diff": "^5.0.0"
    }
}
