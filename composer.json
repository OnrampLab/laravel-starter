{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1.2",
        "calebporzio/parental": "^1.1",
        "guzzlehttp/guzzle": "^7.4",
        "laravel/framework": "^9.0.0",
        "laravel/horizon": "^5.2.1",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "nunomaduro/collision": "^6.1",
        "nwidart/laravel-modules": "^9.0.0",
        "owen-it/laravel-auditing": "^13.0.2",
        "php-open-source-saver/jwt-auth": "^1.4",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.8.0",
        "spatie/laravel-ignition": "^1.0",
        "spatie/laravel-permission": "^5.5.2",
        "symfony/console": "^6.0.0",
        "symfony/finder": "^6.0.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/hello"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "code:analysis": [
            "phpstan analyse"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit"
        ],
        "test:watch": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit-watcher watch --filter NoneTest < /dev/tty"
        ],
        "phpcs:fix-pr": [
            "tools/scripts/phpcs-fix.sh pr"
        ],
        "phpcs:fix-all": [
            "tools/scripts/phpcs-fix.sh all"
        ]
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "beyondcode/laravel-dump-server": "^1.8",
        "fakerphp/faker": "^1.20",
        "filp/whoops": "^2.14",
        "friendsofphp/php-cs-fixer": "^3.13",
        "mockery/mockery": "^1.5",
        "nunomaduro/larastan": "^2.2",
        "onramplab/onr-phpcs-laravel": "^1.2",
        "phpunit/phpunit": "^9.5",
        "sempro/phpunit-pretty-print": "^1.4"
    }
}
